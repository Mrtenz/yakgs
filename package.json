{
  "name": "yet-another-kittens-game-script",
  "version": "1.1.0",
  "description": "A script for Kittens Game",
  "main": "src/index.tsx",
  "repository": "https://github.com/Mrtenz/yakgs",
  "author": "Maarten Zuidhoorn <maarten@zuidhoorn.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "history": "^4.7.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "5.9.1",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-es2017": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@types/history": "^4.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/webpack": "^4.1.3",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "husky": "^0.14.3",
    "imagemin": "^5.3.1",
    "img-loader": "^3.0.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.1.3",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.5",
    "sass-loader": "^7.0.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.1",
    "url-loader": "^1.0.1",
    "userscript-header": "^0.2.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "wrapper-webpack-plugin": "^2.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config ./config/webpack.dev.js --content-base ./dist",
    "tscheck": "tsc --noEmit",
    "tslint": "tslint --project .",
    "prepush": "npm run tscheck",
    "precommit": "lint-staged",
    "prettier:diff": "prettier --write --config ./.prettierrc --list-different \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "precommit": "lint-staged",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  },
  "prepush": "npm run tslint && npm run tscheck",
  "userscript": {
    "name": "Yet Another Kittens Game Script",
    "description": "Some additions to Kittens Game",
    "author": "Morten",
    "namespace": "https://mrten.nl",
    "match": "*bloodrizer.ru/games/kittens*"
  }
}
